Host *
  ControlMaster auto
  ControlPath ~/.ssh/control/%C
  Compression yes
  #CompressionLevel 4
  TCPKeepAlive yes
  ServerAliveInterval 60
  StrictHostKeyChecking no
  Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
  AddKeysToAgent yes
  UseKeyChain yes
